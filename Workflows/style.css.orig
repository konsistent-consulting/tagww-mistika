  text-numBorders: 1;
  text-offsetBorders: 0px 0px 0px 0px;
}

WorkflowCanvas[Dragging1]
{
  color: 1px solid #FFf4f4f4;
}

WorkflowCanvas[Dragging2]
{
  color: 4px solid #FFA3A3A3;
}

WorkflowCanvas[Dragging3]
{
  color: 1px solid #FF686868;
}

/*Workflows Render Queue*/

CqueueWidget QListView, CqueueWidget QListView:focus
{
  background: #141414;
  border:none;
}

CqueueWidget QLabel
{
  color: #d0d0d0;
  font-weight: 500;
  margin-left: 5px;
}

CqueueWidget MPushButton, CqueueWidget QCheckBox 
{
  margin-top: 5px;
  margin-bottom: 5px;
  margin-right: 5px;
}

CqueueItemWidget QLabel
{
  background: transparent;
}

/*Color DCP Report*/

CdcpReportItem[severityText="Ok"] 
{
  color: #42cc5f; 
}

CdcpReportItem[severityText="Info"]
{
  color: #4096df; 
}

CdcpReportItem[severityText="Recommendation"]
{
  color: #f9de5f; 
}

CdcpReportItem[severityText="Warning"]
{
  color: #f78547; 
}

CdcpReportItem[severityText="Critical"]
{
  color: #f23838; 
}

QDockWidget {
    background-color: #BFEFFF;
}










