/*SpinBox*/

QSpinBox, QTreeWidget QSpinBox, QSpinBox:!active, QDoubleSpinBox
{
  border:1px solid #101010;
  padding:1px 4px;
  background: #292929;
  selection-background-color: #6d6d6d;
  selection-color: #000000; 
  border-radius: 0px;
}

QSpinBox::up-button, QDoubleSpinBox::up-button
{
  subcontrol-origin: border;
  subcontrol-position: top right; /* position at the top right corner */
  width: 16px; /* 16 + 2*1px border-width = 15px padding + 3px parent border */
  padding-top: 1px;
  image:url(%1filemanager/button_icons/up_arrow_press.png);
}

QSpinBox::down-button, QDoubleSpinBox::down-button
{
  subcontrol-origin: border;
  subcontrol-position: bottom right; /* position at bottom right corner */
  width: 16px;
  padding-bottom: 1px;
  image:url(%1filemanager/button_icons/down_arrow_press.png);
}

QSpinBox::up-button:hover, QSpinBox::down-button:hover, QDoubleSpinBox::up-button:hover, QDoubleSpinBox::down-button:hover  
{
  background:#404040;  
  border-radius: 2px;
}

QSpinBox:disabled
{
  color: #353535;
  background-color: #141414;
}

QMenu QSpinBox
{
  background: #292929;
  border:none;
  border-radius: none;
}


