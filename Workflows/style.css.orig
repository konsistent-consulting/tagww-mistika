/*QDockWidget*/

QDockWidget
{
  height:100px;
  margin-bottom: 5px;
  color:#d0d0d0;
  titlebar-close-icon:url(%1filemanager/button_icons/close_button.png);
}

QDockWidget::title
{
  text-align: left;
  font-size: 10px;
  background: #292929;
  padding-left: 5px;
  padding-top:4px;
  padding-bottom:4px;
  margin-top: 0px;
  border-top: 1px solid #000000;
  border-right: 1px solid #000000;
  border-left: 1px solid #000000;
}

QDockWidget::close-button 
{
  subcontrol-position: top right;
  subcontrol-origin: margin;
  top: 8px; right: 8px; bottom: 0px;
  width: 15px;
  height: 15px;
  background: #292929;
  border:none;
  /*border-radius: 8px;*/
}

QDockWidget::close-button:hover
{
  background:#404040;
}

QDockWidget::close-button:pressed
{
  padding: 0px;
  background: #000000;
}

