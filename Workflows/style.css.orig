/*QWidget*/

QWidget
{
  font-family: "Open Sans";
  font-size: 12px;
  color: #8a8a8a;
  background: #141414;
  border:0px;

}

/*Splitter*/

QSplitter, QMainWindow::separator, QMainWindow:separator:handle
{
  width: 4px;
  height: 4px;
}

QSplitter::handle:horizontal, QSplitter::handle:vertical, QMainWindow::separator, QMainWindow:separator:handle  
{
  background:#000000;
}


/*Top Menu*/

QMenuBar
{
  padding: 5px 0px 5px 0px;
  background: #101010;
  font-weight: 500;
  spacing:5px;
}

QMenuBar::item {
  background: #101010;
}

QMenuBar::item:selected
{
  color: #42cc5f;
}

/*Right Click Menu*/

QMenu
{
  border:1px solid #1f1f1f;
  background: #303030;
  border-radius: 4px;
}

QMenu::item
{
  background:#303030;
  color: #a3a5a8;

  /*padding: 0px 25px 0px 25px;
  border: 4px solid transparent;*/
}

QMenu::item:disabled
{
  color: #404040;
}

QMenu::item:selected
{
  background:#353535; 
}

QMenu::separator 
{
  height: 1px;
  background: #3a3a3a;
  border-top: 5px solid #303030;
  border-bottom: 5px solid #303030;
}

QMenu::item:checked
{
  color: #42cc5f;
}

/*QMenu::indicator
{
  left:5px;
}

/*QPushButton*/

QPushButton, QPushButton:open, QPushButton:closed
{
  color: #42cc5f;
  padding:5px;
  border:2px solid #000000;
  background: #000000;
  border-radius: 4px;
  outline:none;
}

QPushButton:closed
{
  color:#8a8a8a;
  border-color: #101010;
  background: #292929;
  /*alternative: background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #404040, stop: 0.09 #292929);*/;
}

QPushButton:closed:disabled
{
  color:#000000;
  background: #141414;
}

QPushButton:closed:hover
{
  background: #404040;
}

QPushButton:closed:pressed
{
  background: #000000;
}

QPushButton::menu-indicator {
  width: 5px;
  subcontrol-position: right center;
  subcontrol-origin: padding;
  padding:3px;
  top:2px;
  right: 2px;
}

MPushButton#renderFarm
{
  background: #141414;
  border-color: #141414;
  qproperty-iconSize: 20px;
  margin-right: 2px;
}


/*Toolbars*/

QToolBar
{
  border: 0px;
}

QToolButton
{
  height: 18px;
  width: 18px;
  padding: 4px;
  border:1px solid #141414;
  border-radius: 2px;
  background:#141414;
  color:#9a9a9a;
  margin:2px;
  font-size: 14px;
  qproperty-iconSize: 18px;
}

QToolButton:hover
{
  background: #303030; 
}

QToolButton:checked
{
  background: #000000;
  /*color:#42cc5f;*/
  border:1px solid #353535;
}

QToolButton:pressed
{
  background: #000000;
  border:1px solid #353535;
}

QToolButton::menu-indicator
{
  width: 14px;
  /*subcontrol-position: center;*/
  subcontrol-origin: padding;
  image: iconName("menu-indicator");
}

QToolBar::separator, QToolBar::handle
{
  width: 1px;
  margin: 2px;
  background: #141414;
}

QToolButton:disabled
{
  background: #141414;
  color:#353535;
  border:1px solid #141414;
}


/*Radio Buttons*/
QRadioButton
{
  margin-left: 4px;

}

QRadioButton::indicator::unchecked
{
  image: url(%1filemanager/button_icons/rbutton_uncheck.png);
  width: 16px;
  height: 16px; 
}

QRadioButton::indicator:unchecked:hover {
  image: url(%1filemanager/button_icons/rbutton_hover.png);
  width: 16px;
  height: 16px;
}

QRadioButton::indicator::checked {
  image: url(%1filemanager/button_icons/rbutton_check.png);
  width: 16px;
  height: 16px;
}


/*ScrollBar*/

QScrollBar:vertical
{
  background:#141414;
  width: 8px;
  padding: 1px;
  border-radius: 4px;
}

QScrollBar::handle:vertical 
{
  background: #404040;
  min-height:6px;
  border-radius: 3px;
}

QScrollBar::handle:vertical:hover 
{
  background: #505050;
  min-height: 6px;
}
 
QScrollBar::add-line:vertical, QScrollBar::add-line:vertical:hover, QScrollBar::sub-line:vertical, QScrollBar::sub-line:vertical:hover, QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical, QScrollBar::add-line:horizontal:hover, QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal, QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal   
{
  border: none;
  background: none;
  height:0px;
}

QScrollBar:horizontal
{
  background:#141414;
  height: 8px;
  padding: 1px;
  border-radius: 4px;
}

QScrollBar::handle:horizontal 
{
  background: #404040;
  min-width: 6px;
  border-radius: 3px;
}

QScrollBar::handle:horizontal:hover 
{
  background: #505050;
  min-width: 12px;
}

/*GroupBox*/

QGroupBox 
{
  background-color: #141414;
  border-top: 2px solid qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0, stop: 0 #404040, stop: 0.7 #141414);
  margin-top: 12px; /* leave space at the top for the title */
  padding-top: 12px;
  outline: none;
}

QGroupBox::title 
{
  subcontrol-origin: margin;
  subcontrol-position: top left; /* position at the top center */
  padding: 0 5px;
  left:0px;
  color: #d0d0d0;
  outline: none;
}


/*ComboBox*/

QComboBox, QDateTimeEdit
{
  height: 20px;
  background: #292929;
  padding-left: 4px;
  padding-right: 4px;
  selection-background-color: #404040;
  selection-color: #d0d0d0;
  border: 1px solid #101010;
  border-radius: 2px;
}

QComboBox:disabled
{
  background: #161616;
  color:#353535;
}

QComboBox::drop-down, QComboBox::drop-down:hover, QDateTimeEdit::drop-down, QDateTimeEdit::drop-down:hover
{
  subcontrol-origin: padding;
  subcontrol-position: top right;
  width: 20px;
  border-left: 0px solid #292929;
}

QComboBox::drop-down:hover, QDateTimeEdit::drop-down:hover, QComboBox:hover
{
  background: #404040;
}

QComboBox::down-arrow, QDateTimeEdit::down-arrow 
{
  image:url(%1filemanager/button_icons/down_arrow_press.png);
  width: 15px;
  height: 15px;
}

QComboBox::down-arrow:hover, QDateTimeEdit::down-arrow:hover
{
  image:url(%1filemanager/button_icons/down_arrow_press.png);
  width: 15px;
  height: 15px;
}


QComboBox QListView, QComboBox QListView:focus
{
  background: #292929;
  border:1px solid #101010;
  outline: 0px;
}

QComboBox QAbstractItemView 
{
  selection-background-color: #404040;
}

