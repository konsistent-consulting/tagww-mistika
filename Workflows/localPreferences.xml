<?xml version="1.0" encoding="UTF-8"?>
<systemSetup>
  <audio>
    <render>
      <baseUSID>ESSGO</baseUSID>
    </render>
  </audio>
  <codecs>
    <arri>
      <cpuMemory>1024</cpuMemory>
      <decodeThreads>8</decodeThreads>
      <decoderType>0</decoderType>
      <gpuMemory>2048</gpuMemory>
      <reduceFactor>1</reduceFactor>
    </arri>
    <canon>
      <reduceFactor>1</reduceFactor>
    </canon>
    <exr>
      <optimizeRead>MULTI_LAYER</optimizeRead>
    </exr>
    <red>
      <loadRMDFileByDefault>true</loadRMDFileByDefault>
      <r3dPerformance>
        <autoadjustPerfValues>true</autoadjustPerfValues>
        <cpuFrames>0</cpuFrames>
        <cpuMemKb>1024</cpuMemKb>
        <cpuThreads>0</cpuThreads>
        <decoderType>3</decoderType>
        <gpuFrames>2</gpuFrames>
        <gpuMemKb>4096</gpuMemKb>
      </r3dPerformance>
      <reduceFactor>1</reduceFactor>
    </red>
    <sony>
      <reduceFactor>1</reduceFactor>
    </sony>
  </codecs>
  <generalSystem>
    <imageProviderLoaderEnabled>false</imageProviderLoaderEnabled>
    <initSDKS512GBRRAM>true</initSDKS512GBRRAM>
    <useIconProviderCache>true</useIconProviderCache>
    <useImageProviderCache>true</useImageProviderCache>
    <useMediaBrowserImagesMaker>true</useMediaBrowserImagesMaker>
  </generalSystem>
  <import>
    <searchStereo>true</searchStereo>
    <stereoLeftPattern>LEFT</stereoLeftPattern>
    <stereoRightPattern>RIGHT</stereoRightPattern>
  </import>
  <interface>
    <nodeConnections>
      <connectionCurvature>10</connectionCurvature>
      <connectionQuality>3</connectionQuality>
    </nodeConnections>
    <screens>
      <printScreensInfo>false</printScreensInfo>
      <scaleForScreens>0.00;1.00</scaleForScreens>
      <scaleScreensFactorsOnRuntime>false</scaleScreensFactorsOnRuntime>
      <useScale>true</useScale>
    </screens>
  </interface>
  <networkListener>
    <ip/>
    <port>8889,8890,8891,8892,8893,8894,8895,8896,8897,8898,8899</port>
  </networkListener>
  <performance>
    <MinimizeGPURAMUsage>false</MinimizeGPURAMUsage>
    <backloadBuffers>true</backloadBuffers>
    <encodeThreads>4</encodeThreads>
    <ioThreads>4</ioThreads>
    <lowResRatio>4</lowResRatio>
    <maxCacheMemory>4900</maxCacheMemory>
    <maxGPUMemory>-1</maxGPUMemory>
    <parallelCpuCores>24</parallelCpuCores>
    <pipeUnits>8</pipeUnits>
    <playBufferThreshold>10</playBufferThreshold>
    <playoutRingBuffer>18</playoutRingBuffer>
    <renderUnits>4</renderUnits>
    <sanFilePreallocation>false</sanFilePreallocation>
  </performance>
  <render>
    <batch>
      <path>/Users/ed/SGO Data/Projects/_BATCH_QUEUES</path>
    </batch>
    <multiGPU>
      <nvidiaMultiGPU>false</nvidiaMultiGPU>
    </multiGPU>
    <totem>
      <clusterInstances>1</clusterInstances>
      <clusterParallel>24</clusterParallel>
      <clusterPlaybackTailFrames>24</clusterPlaybackTailFrames>
      <clusterPlaybackTailMiliseconds>2000</clusterPlaybackTailMiliseconds>
      <clusterRenderUnits>5</clusterRenderUnits>
      <useTotem>false</useTotem>
    </totem>
  </render>
  <renderFarm>
    <farm>
      <deadline>
        <farmParams/>
        <path/>
        <submissionPath/>
      </deadline>
      <smedge>
        <packetSize>100</packetSize>
        <path/>
      </smedge>
    </farm>
    <farmUsed>0</farmUsed>
  </renderFarm>
  <streaming>
    <use>false</use>
  </streaming>
  <tabletDevice>
    <maxPressure>1024</maxPressure>
    <minMoveThreshold>4</minMoveThreshold>
    <minPressure>0</minPressure>
  </tabletDevice>
  <videoBoard>
    <autodetect>false</autodetect>
    <model>DVS</model>
    <use>false</use>
  </videoBoard>
  <vtr>
    <captureCorrection>0</captureCorrection>
    <dumpCorrection>0</dumpCorrection>
    <editLag>0</editLag>
    <freeTC>false</freeTC>
    <maxFree>01:00:00:00</maxFree>
    <postHandle>7</postHandle>
    <preHandle>7</preHandle>
    <preRoll>00:00:05:00</preRoll>
  </vtr>
</systemSetup>
