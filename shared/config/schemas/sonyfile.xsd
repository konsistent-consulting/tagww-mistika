<xsd:schema xmlns:sgo="http://www.sgo.es/2017/sgo.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <xsd:element name="SonyFile" sgo:categoryType="categoryCollapsible" sgo:visibility="0" sgo:sortOrder="0">
