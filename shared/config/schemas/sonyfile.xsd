                <xsd:element name="sonyGamut" type="stringPulldown" sgo:visibility="0" sgo:sortOrder="8" sgo:actionType="getDataFromProperty" sgo:actionTypeProperties="_sonyGamutList"/>
                <xsd:element name="gamut" type="label" sgo:visibility="0" sgo:sortOrder="9"/>
                <xsd:element name="sonyISO" type="stringPulldown" sgo:visibility="0" sgo:sortOrder="10" sgo:actionType="getDataFromProperty" sgo:actionTypeProperties="_sonyISOList"/>
