                <xsd:element name="nodeLocked" type="xsd:boolean" sgo:visibility="0" sgo:sortOrder="3"/>
                <xsd:element name="debayer" type="integerPulldown" sgo:integerPulldownValues="From File$$$0|Custom$$$1" sgo:visibility="0" sgo:sortOrder="4"/>
                <xsd:element name="sourceGamut" type="integerPulldown" sgo:integerPulldownValues="From File$$$-1|S-Gamut$$$0|S-Gamut3$$$1|S-Gamut3.cine$$$2" sgo:visibility="0" sgo:sortOrder="5"/>
