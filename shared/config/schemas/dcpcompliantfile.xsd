				<xsd:element name="scaleFilter" type="integerPulldown" sgo:integerPulldownValues="From Input$$$-1|Nearest$$$0|Bilinear$$$1|Bicubic$$$2|7 Taps$$$3|15 Taps$$$4" sgo:visibility="0" sgo:sortOrder="11"/>
