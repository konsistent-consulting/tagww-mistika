                <xsd:element name="filterRadY" type="xsd:decimal" sgo:decimalMinValue="1" sgo:decimalMaxValue="64" sgo:visibility="0" sgo:sortOrder="12"/>
                <xsd:element name="filterPY" type="xsd:decimal" sgo:decimalMinValue="0" sgo:decimalMaxValue="64" sgo:visibility="0" sgo:sortOrder="13"/>
                <xsd:element name="filterRadC" type="xsd:decimal" sgo:decimalMinValue="1" sgo:decimalMaxValue="64" sgo:visibility="0" sgo:sortOrder="14"/>
                <xsd:element name="filterPC" type="xsd:decimal" sgo:decimalMinValue="0" sgo:decimalMaxValue="64" sgo:visibility="0" sgo:sortOrder="15"/>
                <xsd:element name="filterChroma" type="integerPulldown" sgo:integerPulldownValues="None$$$0|3x3$$$1|5x5$$$2|7x7$$$3|9x9$$$4|11x11$$$5|13x13$$$6|15x15$$$7|17x17$$$8" sgo:visibility="0" sgo:sortOrder="16"/>
