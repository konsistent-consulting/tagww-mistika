                <xsd:element name="redGain" type="xsd:decimal" sgo:decimalMinValue="0" sgo:decimalMaxValue="100000" sgo:visibility="0" sgo:sortOrder="18"/>
                <xsd:element name="blueGain" type="xsd:decimal" sgo:decimalMinValue="0" sgo:decimalMaxValue="100000" sgo:visibility="0" sgo:sortOrder="19"/>
                <xsd:element name="changeisoGain" type="xsd:boolean" sgo:visibility="0" sgo:sortOrder="20"/>
                <xsd:element name="isoGain" type="xsd:decimal" sgo:decimalMinValue="0" sgo:decimalMaxValue="100000" sgo:visibility="0" sgo:sortOrder="21"/>
                <xsd:element name="colorMatrix" type="integerPulldown" sgo:integerPulldownValues="Source$$$0|None$$$1" sgo:visibility="0" sgo:sortOrder="22"/>
                <xsd:element name="amount" type="xsd:decimal" sgo:decimalMinValue="0" sgo:decimalMaxValue="100000" sgo:visibility="0" sgo:sortOrder="23"/>
                <xsd:element name="threshold" type="xsd:decimal" sgo:decimalMinValue="0" sgo:decimalMaxValue="100" sgo:visibility="0" sgo:sortOrder="24"/>
                <xsd:element name="NameConvention" sgo:categoryType="categoryCollapsible" sgo:visibility="0" sgo:sortOrder="25">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="addTimeStamp" type="xsd:boolean" sgo:visibility="0" sgo:sortOrder="0"/>
                            <xsd:element name="nameConvention" type="nameConventionSimpleEditor" sgo:visibility="0" sgo:sortOrder="1"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="metadata" type="mediaFileInfoWidget" sgo:visibility="0" sgo:sortOrder="26"/>
            </xsd:sequence>
        </xsd:complexType>
