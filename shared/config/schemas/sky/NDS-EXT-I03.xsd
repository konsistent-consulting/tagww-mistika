<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="URN:NNDS:CMS:ADI3:01" xmlns:xml="http://www.w3.org/XML/1998/namespace" xmlns:content="http://www.cablelabs.com/namespaces/metadata/xsd/content/1" xmlns:core="http://www.cablelabs.com/namespaces/metadata/xsd/core/1" targetNamespace="URN:NNDS:CMS:ADI3:01" elementFormDefault="qualified" attributeFormDefault="unqualified" version="3">
	<!-- Import for xml:lang and xml:space -->
	<xsd:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml_2001.xsd"/>
	<xsd:import namespace="http://www.cablelabs.com/namespaces/metadata/xsd/content/1" schemaLocation="MD-SP-CONTENT-I01.xsd"/>
	<xsd:import namespace="http://www.cablelabs.com/namespaces/metadata/xsd/core/1" schemaLocation="MD-SP-CORE-I01.xsd"/>
	<xsd:complexType name="PressPackImageType">
		<xsd:annotation>
			<xsd:documentation>Defines a still image which can be used for any purpose.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="content:StillImageAssetType">
				<xsd:sequence>
					<xsd:element name="Usage" type="UsageType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:simpleType name="ZeroToNinetyNineType">
		<xsd:restriction base="xsd:unsignedByte">
			<xsd:minInclusive value="0"/>
			<xsd:maxInclusive value="99"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Max65535Type">
		<xsd:restriction base="xsd:unsignedInt">
			<xsd:minInclusive value="0"/>
			<xsd:maxInclusive value="65535"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="MaxStringFourZeroNineSixType">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="4096"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="MaxStringTwentyType">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="20"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AnyURLMax2000Type">
		<xsd:restriction base="xsd:anyURI">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="2000"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TermTypeType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="CUTV"/>
			<xsd:enumeration value="Archive"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ContentGroupTypeType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="AV"/>
			<xsd:enumeration value="BoxSet"/>
			<xsd:enumeration value="Show"/>
			<xsd:enumeration value="Season"/>
			<xsd:enumeration value="PP"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="ContainerExtType">
		<xsd:annotation>
			<xsd:documentation>This contains NDS extensions for the container</xsd:documentation>
		</xsd:annotation>
		<xsd:choice maxOccurs="unbounded">
			<xsd:element name="CategoryImage" type="CategoryImageType" minOccurs="0"/>
			<xsd:element name="PressPackImage" type="PressPackImageType"/>
			<xsd:element name="SecondaryContent" type="SecondaryContentType"/>
			<xsd:element name="GroupMember" type="GroupMemberType"/>
			<!-- 8.1.0 addition -->
			<xsd:element name="AudioLayout" type="AudioLayoutType" minOccurs="0" maxOccurs="1"/>
			<!-- 8.1.0 addition -->
			<xsd:element name="ClosedCaptions" type="ClosedCaptionsType" minOccurs="0" maxOccurs="1"/>
		</xsd:choice>
	</xsd:complexType>
	<xsd:simpleType name="PlatformType">
		<xsd:annotation>
			<xsd:documentation>A 10-character string representing a platform code.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="core:NonEmptyStringType">
			<xsd:maxLength value="10"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="EpisodeBriefType">
		<xsd:annotation>
			<xsd:documentation>A 19-character string representing the briefest displayable title.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="core:NonEmptyStringType">
			<xsd:maxLength value="19"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="EpisodeNameType">
		<xsd:annotation>
			<xsd:documentation>A 40-character string representing a reasonable length displayable episode name.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="core:NonEmptyStringType">
			<xsd:maxLength value="40"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="EpisodeLongType">
		<xsd:annotation>
			<xsd:documentation>A 200-character string representing a full displayable title.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="core:NonEmptyStringType">
			<xsd:maxLength value="200"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SummaryBriefType">
		<xsd:annotation>
			<xsd:documentation>An 150-character string representing the briefest displayable summary.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="core:NonEmptyStringType">
			<xsd:maxLength value="150"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SummaryShortType">
		<xsd:annotation>
			<xsd:documentation>A 256-character string representing the shortest displayable summary.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="core:NonEmptyStringType">
			<xsd:maxLength value="256"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SummaryMediumType">
		<xsd:annotation>
			<xsd:documentation>A 1000-character string representing a reasonable length displayable summary.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="core:NonEmptyStringType">
			<xsd:maxLength value="1000"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SummaryLongType">
		<xsd:annotation>
			<xsd:documentation>A 4096-character string representing a full displayable summary.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="core:NonEmptyStringType">
			<xsd:maxLength value="4096"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SeasonBriefType">
		<xsd:annotation>
			<xsd:documentation>A 19-character string representing the briefest displayable title.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="core:NonEmptyStringType">
			<xsd:maxLength value="19"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SeasonMediumType">
		<xsd:annotation>
			<xsd:documentation>A 35-character string representing a reasonable length displayable episode name.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="core:NonEmptyStringType">
			<xsd:maxLength value="35"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SeasonLongType">
		<xsd:annotation>
			<xsd:documentation>A 128-character string representing a full displayable title.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="core:NonEmptyStringType">
			<xsd:maxLength value="128"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="MarketingMessageType">
		<xsd:annotation>
			<xsd:documentation>A 200-character string representing a marketing message.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="core:NonEmptyStringType">
			<xsd:maxLength value="200"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="KeyType">
		<xsd:annotation>
			<xsd:documentation>A 20-character key.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="core:NonEmptyStringType">
			<xsd:maxLength value="20"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SoundmouseType">
		<xsd:annotation>
			<xsd:documentation>A 20-character string representing a Soundmouse ID.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="core:NonEmptyStringType">
			<xsd:maxLength value="20"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="StudioCodeType">
		<xsd:annotation>
			<xsd:documentation>A 8-character string representing a Studio Code.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="core:NonEmptyStringType">
			<xsd:maxLength value="8"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ServiceKeyType">
		<xsd:annotation>
			<xsd:documentation>A key uniquely identifying a service.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:integer">
			<xsd:minInclusive value="0"/>
			<xsd:maxInclusive value="9999"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Mode3dType">
		<xsd:restriction base="xsd:unsignedByte">
			<xsd:enumeration value="3"/>
			<!--			<xsd:enumeration value="0"/> -->
			<!--			<xsd:enumeration value="1"/> -->
			<!--			<xsd:enumeration value="2"/> -->
			<!--			<xsd:enumeration value="4"/> -->
			<!-- 0 - Each component plane of the decoded frames contains a "checkerboard" based interleaving of corresponding planes of two constituent frames. -->
			<!-- 1- Each component plane of the decoded frames contains a column-based interleaving of corresponding planes of two constituent frames. -->
			<!-- 2 - Each component plane of the decoded frames contains a row-based interleaving of corresponding planes of two constituent frames.  -->
			<!-- 3 - Each component plane of the decoded frames contains a side-by-side packing arrangement of corresponding planes of two constituent frames. -->
			<!-- 4 - Each component plane of the decoded frames contains top-bottom packing arrangement of corresponding planes of two constituent frames. -->
			<!-- 5 - The component planes of the decoded frames in output order form a temporal interleaving of alternating first and second constituent frames.  -->
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="ServiceType">
		<xsd:annotation>
			<xsd:documentation>Used to define service CUTV event is broadcast on</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="seriesId" type="KeyType" use="optional"/>
		<xsd:attribute name="serviceKey" type="ServiceKeyType" use="required"/>
	</xsd:complexType>
	<xsd:simpleType name="DateTimeMinuteTimezoneType">
		<xsd:annotation>
			<xsd:documentation>A standard xsd:dateTime that requires a timezone indicator and no seconds.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:dateTime">
			<xsd:pattern value="[0-9]{4}\-[0-9]{2}\-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}(Z|([\-+][0-9]+:[0-9]+))"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="CutvEventType">
		<xsd:sequence>
			<xsd:element name="Service" type="ServiceType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The service the CUTV was broadcast on, and the seriesKey</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="epgDateTime" type="DateTimeMinuteTimezoneType" use="required">
			<xsd:annotation>
				<xsd:documentation>Is the date and time the EPG presented the title as being broadcast.  This shall be rounded to the minute.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="leadServiceKey" type="ServiceKeyType" use="required">
			<xsd:annotation>
				<xsd:documentation>Identifies the service branding used to present the asset in the on-demand catalogue</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:simpleType name="seasonNumType">
		<xsd:restriction base="xsd:unsignedInt">
			<xsd:minInclusive value="1"/>
			<xsd:maxInclusive value="65535"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="episodeNumType">
		<xsd:restriction base="xsd:unsignedInt">
			<xsd:minInclusive value="1"/>
			<xsd:maxInclusive value="32767"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="totalEpisodeNumType">
		<xsd:restriction base="xsd:unsignedInt">
			<xsd:minInclusive value="1"/>
			<xsd:maxInclusive value="32767"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="SeriesInfoType">
		<xsd:sequence>
			<xsd:element name="SummaryBrief" type="SummaryBriefType" minOccurs="0"/>
			<xsd:element name="SummaryShort" type="SummaryShortType" minOccurs="0"/>
			<xsd:element name="SummaryMedium" type="SummaryMediumType" minOccurs="0"/>
			<xsd:element name="SummaryLong" type="SummaryLongType" minOccurs="0"/>
			<xsd:element name="SeasonInfo" type="SeasonInfoType" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="seasonNumber" type="seasonNumType" use="required">
			<xsd:annotation>
				<xsd:documentation>The season number for the series the title is an episode of.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="episodeNumber" type="episodeNumType" use="required">
			<xsd:annotation>
				<xsd:documentation>The episode number of the title in the series.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="totalEpisodes" type="totalEpisodeNumType" use="optional">
			<xsd:annotation>
				<xsd:documentation>The total number of episodes in the series, if known.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<!-- 8.1.0 addition -->
		<xsd:attribute ref="xml:lang" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					<div>					
						<h3>lang (as an attribute name)</h3>
						<p>Denotes the language of the series info that conforms to the 3 character ISO 639-2 standard.
							Should have a corresponding LocalizableTitle tag with matching lang attribute.
						</p>						
					</div>
				</xsd:documentation>				
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="SeasonInfoType">
		<xsd:sequence>
			<xsd:element name="SummaryBrief" type="SummaryBriefType" minOccurs="0"/>
			<xsd:element name="SummaryShort" type="SummaryShortType" minOccurs="0"/>
			<xsd:element name="SummaryMedium" type="SummaryMediumType" minOccurs="0"/>
			<xsd:element name="SummaryLong" type="SummaryLongType" minOccurs="0"/>
			<xsd:element name="SeasonBrief" type="SeasonBriefType" minOccurs="0"/>
			<xsd:element name="SeasonMedium" type="SeasonMediumType" minOccurs="0"/>
			<xsd:element name="SeasonLong" type="SeasonLongType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="LocalizableTitleExtType">
		<xsd:annotation>
			<xsd:documentation>A collection of extended Title metadata which is specified in multiple languages for localization.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="EpisodeLong" type="EpisodeLongType" minOccurs="0"/>
			<xsd:element name="EpisodeBrief" type="EpisodeBriefType" minOccurs="0"/>
			<xsd:element name="EpisodeName" type="EpisodeNameType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The name of the individual episode of a series.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MarketingMessage" type="MarketingMessageType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The marketing message used to promote the content</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute ref="xml:lang" use="required"/>
	</xsd:complexType>
	<xsd:simpleType name="LinkedObjectTypeType">
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
	<xsd:simpleType name="LinkedObjectObjectType">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="100"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="LinkedObjectType">
		<xsd:annotation>
			<xsd:documentation>Used to define a linkage between an asset and an object</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="type" type="LinkedObjectTypeType" use="required"/>
		<xsd:attribute name="object" type="LinkedObjectObjectType" use="required"/>
		<xsd:attribute name="orderNum" type="Max65535Type" use="optional"/>
	</xsd:complexType>
	<xsd:complexType name="OfferCapabilityType">
		<xsd:sequence>
			<xsd:element name="IsStreamable" type="xsd:boolean"/>
			<xsd:element name="IsDownloadable" type="xsd:boolean"/>
		</xsd:sequence>
	</xsd:complexType>
	

	<!-- 8.1.0 addition  -->
	<xsd:complexType name="FulfilmentOptionType">		
		<xsd:simpleContent>
			<xsd:extension base="TextMax50Type">
				<xsd:attribute name="code" use="optional" type="Max65535Type"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	
	<!-- 8.1.0 addition  -->
	<xsd:complexType name="FullFilmentGroupType">
		<xsd:sequence>
			<xsd:element name="FulfilmentDescriptor" type="TextMax4096Type" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Description of the fulfilment group.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FulfilmentOption" type="FulfilmentOptionType" minOccurs="1" maxOccurs="unbounded"/>				
		</xsd:sequence>
		<xsd:attribute name="productCode" type="Max65535Type" use="optional"/>
		<xsd:attribute name="rank" type="ZeroToNinetyNineType" use="optional"/>
	</xsd:complexType>
	
	<!-- 8.1.0 addition  -->
	<xsd:complexType name="OfferPropositionType">
		<xsd:sequence>
			<xsd:element name="FullFilmentGroup" type="FullFilmentGroupType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Groups one or more fulfilment options.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="name" type="TextMax20Type" use="required"/>
		<xsd:attribute name="code" type="Max65535Type" use="optional"/>		
	</xsd:complexType>
	
	<xsd:complexType name="NdsAssetExtType">
		<xsd:annotation>
			<xsd:documentation>This contains NDS extensions for the Assets</xsd:documentation>
		</xsd:annotation>
		<xsd:choice minOccurs="0" maxOccurs="unbounded">
			<xsd:element name="TermType" type="TermTypeType">
				<xsd:annotation>
					<xsd:documentation>Identifies the type of Term Asset.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Extensions for Offer Asset -->
			<xsd:element name="platform" type="PlatformType"/>
			<xsd:element name="CutvEvent" type="CutvEventType">
				<xsd:annotation>
					<xsd:documentation>Identifies if the Offer is for a CUTV Event and provides metadata about the broadcast event.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OfferAvailability" type="OfferAvailabilityType"/>
			<!-- Extensions for Content Group Asset -->
			<xsd:element name="OfferCapability" type="OfferCapabilityType"/>
			
			<!-- 8.1.0 addition  -->
			
			<xsd:element name="OfferProposition" type="OfferPropositionType" nillable="false">
				<xsd:annotation>
					<xsd:documentation>Identifies the type of proposition for an offer</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<xsd:element name="ContentGroupType" type="ContentGroupTypeType">
				<xsd:annotation>
					<xsd:documentation>Identifies the type of content group.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CategoryImageRef" type="core:AssetRefType"/>
			<xsd:element name="PressPackImageRef" type="core:AssetRefType"/>
			<xsd:element name="SecondaryContentRef" type="core:AssetRefType"/>
			<xsd:element name="GroupMemberRef" type="core:AssetRefType"/>
			
			<!-- 8.1.0 addition  -->
			<xsd:element name="AudioLayoutRef" type="core:AssetRefType">
				<xsd:annotation>
					<xsd:documentation>
						<div>					
							<h3>AudioLayoutRef</h3>
							<p>Reference to an Audio Layout</p>						
						</div>
					</xsd:documentation>				
				</xsd:annotation>
			</xsd:element>
			
			<!-- 8.1.0 addition  -->
			<xsd:element name="ClosedCaptionsRef" type="core:AssetRefType">
				<xsd:annotation>
					<xsd:documentation>
						<div>					
							<h3>AudioLayoutRef</h3>
							<p>Reference to Closed Captions</p>						
						</div>
					</xsd:documentation>				
				</xsd:annotation>
			</xsd:element>
			
			<!-- Extensions for Title Asset -->
			<xsd:element name="LocalizableTitleExt" type="LocalizableTitleExtType">
				<xsd:annotation>
					<xsd:documentation>Extension to the Localizable title.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SeriesInfo" type="SeriesInfoType">
				<xsd:annotation>
					<xsd:documentation>Information about the series the title is an episode of.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IsSigned" type="xsd:boolean"/>
			<xsd:element name="IsSubtitle" type="xsd:boolean"/>
			<xsd:element name="IsAudioDescribed" type="xsd:boolean"/>
			<xsd:element name="ExhibitionDateTime" type="core:DateTimeTimezoneType">
				<xsd:annotation>
					<xsd:documentation>First time the title was shown on linear broadcast.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Soundmouse" type="SoundmouseType"/>
			<xsd:element name="StudioCode" type="StudioCodeType"/>
			<xsd:element name="LinkedObject" type="LinkedObjectType"/>
			<xsd:element name="TitleExtensions" type="TitleExtensionType"/>
			<xsd:element name="TitleRegionalExtensions" type="TitleRegionalExtensionsType"/>
			<!-- Extensions for Movie Asset -->
			<xsd:element name="Mode3d" type="Mode3dType">
				<xsd:annotation>
					<xsd:documentation>Required if content is stereoscopic 3D.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MovieExtensions" type="MovieExtensionType"/>
			<!-- restrict string to 6 Alpha characters -->
			<xsd:element name="PreviewExtensions" type="PreviewExtensionType"/>
			<xsd:element name="ThumbnailExtensions" type="ThumbnailExtensionType"/>
			<xsd:element name="CategoryAssetExtensions" type="CategoryAssetExtensionType"/>
			<xsd:element name="TermCategoryTags" type="TermCategoryTagsType"/>
			
			<!-- 8.1.0 deprecation -->
			<xsd:element name="TermRightsSummary" type="RightsSummaryType" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Depcrecated</xsd:documentation>				
				</xsd:annotation>
			</xsd:element>
			
			<!-- 8.1.0 addition: multiplicity must be enforced by business rules -->
			<xsd:element name="Region" type="ISO3166CountryCode" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						<div>					
							<h3>Region</h3>
							<p>The region/country (alpha-2) available for given rights/platform in the ADI
								<a href="http://www.iso.org/iso/country_codes">
									http://www.iso.org/iso/country_codes</a> Two character country code </p>						
						</div>
					</xsd:documentation>				
				</xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<xsd:simpleType name="TextMax4096Type">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="4096"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TextMax20Type">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="20"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TextMax50Type">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="50"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="ADSMetadataType">
		<xsd:simpleContent>
			<xsd:extension base="TextMax4096Type">
				<xsd:attribute name="adsLogicalName" type="TextMax20Type" use="optional"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:simpleType name="EventTriggerType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="impression"/>
			<xsd:enumeration value="start"/>
			<xsd:enumeration value="end"/>
			<xsd:enumeration value="playBackTerminated"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="ReportingURLType">
		<xsd:simpleContent>
			<xsd:extension base="AnyURLMax2000Type">
				<xsd:attribute name="eventTrigger" type="EventTriggerType" use="required"/>
				<xsd:attribute name="assetPartNum" type="xsd:unsignedByte" use="optional"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="AssetPartType">
		<xsd:annotation>
			<xsd:documentation>Used to represent parts of an asset. The standard element are used for the first part and subsequent parts are specified using this type</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="SourceUrl" type="content:SourceUrlType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A URL to the content, which ni ADI1.1 corresponds to a relatative URL of just the content file name.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ContentFileSize" type="xsd:unsignedLong" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>File size (in bytes) of the included content for quality assurance and processing.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ContentCheckSum" type="content:ChecksumType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A string containing a hexadecimal number representing a MD5 (RFC 1321) message digest value for quality assurance.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SOE" type="core:TimecodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The start time code of the beginning of the content file</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SOM" type="core:TimecodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The start time code of the part in the content file</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EOM" type="core:TimecodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The end time code of the the part in the content file</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- 8.1.0 addition (merged from 7.13)  -->
			<xsd:element name="SOC" type="core:TimecodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The time code of the start of credit roll in the content file</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="partNum" type="xsd:unsignedByte" use="required"/>
	</xsd:complexType>
	<xsd:simpleType name="MediaIdType">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="20"/>
		</xsd:restriction>
